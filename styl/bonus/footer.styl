footer
	padding 20px 0
	background-color: #dfc194
	text-align: center
	h5
		font-size: 20px;
		color: #000
		margin 0
		font-family: "YanoneKaffeesatzBold", sans-serif;
	p
		font-size: 14px;
		color: rgb(0, 0, 0);
		line-height: 1.786;
		font-family: "AvenirNextCyr", sans-serif
.soc_buttons
	width 100%
	margin: 0 auto
	a
		display: inline-block
		margin: 10px 7px 15px
		position relative
		z-index: 1
		.fa
			border-radius: 50px
			background-color: #fff
			border: 1px solid rgb(0,86,194)
			width: 32px;
			height: 32px;
			color #dfc194
			line-height 32px
			font-size 18px
			position relative
			z-index 9
			transition all .3s ease
		&:hover
			.fa
				background-color: rgb(0,86,194)
				color #fff