#get
#for
	padding 50px 0
	background-color: rgb(0,0,0)
	background-image url(../img/get_bg_1.png), url(../img/get_bg_2.png)
	background-repeat no-repeat, no-repeat
	background-position: center top, center bottom
.get_list
	width 100%
	max-width 850px
	background-color: rgb(219,182,139)
	background-image: linear-gradient( 90deg, rgb(223,193,148) 0%, rgb(187,163,127) 100%);
	box-shadow: 0px 3px 16.38px 1.62px rgba(0, 0, 0, 0.39);
	padding 22px 40px
	margin 0 auto 50px
	border-radius: 10px
	li
		font-size: 20px
		margin-bottom: 10px
		&:last-child
			margin-bottom: 0